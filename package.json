		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"express-session": "^1.17.2",
		"if-env": "^1.0.4",
		"jsonwebtoken": "^8.5.1",
		"mysql2": "^2.2.5",
		"sequelize": "^6.6.5"
	},
	"devDependencies": {
		"eslint": "^7.12.1",
